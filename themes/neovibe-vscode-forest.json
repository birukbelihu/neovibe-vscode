        "textLink.foreground": "#39FF14",
        "textLink.activeForeground": "#00F5FF",
        "progressBar.background": "#39FF14"
    },
    "tokenColors": [
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#39FF14",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#00F5FF"
            }
        },
        {
            "scope": "string.regexp",
            "settings": {
                "foreground": "#FFD700"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#39FF14"
            }
        },
        {
            "scope": "function",
            "settings": {
                "foreground": "#00F5FF"
            }
        },
        {
            "scope": "comment",
            "settings": {
                "foreground": "#6E996E",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#FFD700"
            }
        },
        {
            "scope": "type",
            "settings": {
                "foreground": "#39FF14"
            }
        },
        {
            "scope": "support.function",
            "settings": {
                "foreground": "#00F5FF"
            }
        },
        {
            "scope": "punctuation",
            "settings": {
                "foreground": "#39FF14"
            }
        },
        {
            "scope": "operator",
            "settings": {
                "foreground": "#00F5FF"
            }
        }
    ]
}