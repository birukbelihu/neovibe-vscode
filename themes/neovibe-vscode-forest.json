        "gitDecoration.untrackedResourceForeground": "#00F5FF",
        "dropdown.background": "#0A1F0A",
        "dropdown.foreground": "#39FF14",
        "dropdown.listBackground": "#071807",
        "dropdown.listForeground": "#E0E0E0",
