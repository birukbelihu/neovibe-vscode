        "inputOption.activeBackground": "#39FF1433",
        "inputOption.activeBorder": "#39FF14",
        "inputOption.activeForeground": "#39FF14",
        "inputValidation.errorBackground": "#1A0A0A",
        "inputValidation.errorBorder": "#FF3C38",
        "inputValidation.warningBackground": "#1A1A0A",
        "inputValidation.warningBorder": "#FFD700",
        "inputValidation.infoBackground": "#0A1A1A",
        "inputValidation.infoBorder": "#00F5FF",
        "editorWidget.background": "#0A1F0A",
        "editorWidget.border": "#39FF14",
