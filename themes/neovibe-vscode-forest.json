        "button.foreground": "#39FF14",
        "button.hoverBackground": "#39FF1433",
        "button.secondaryBackground": "#071807",
        "button.secondaryHoverBackground": "#00F5FF33",
        "input.background": "#0D260D",
        "input.foreground": "#39FF14",
