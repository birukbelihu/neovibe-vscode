{
  "name": "NeoVibe Sunset",
  "author": "Biruk Belihu",
  "type": "dark",
  "colors": {
    "editor.background": "#1B0B2A",
    "editor.foreground": "#E6E1E8",

    "editorCursor.foreground": "#FF3CA6",
    "editorCursor.background": "#FF5F7F33",

    "editor.lineHighlightBackground": "#2E1638",

    "editor.selectionBackground": "#FF5F7F66",
    "editor.selectionHighlightBackground": "#FFD75F55",

    "editorBracketMatch.background": "#FF5F7FCC",
    "editorBracketMatch.border": "#FF3CA6",

    "editorBracketHighlight.foreground1": "#FF3CA6",
    "editorBracketHighlight.foreground2": "#FFD75F",
    "editorBracketHighlight.foreground3": "#FF7FA5",
    "editorBracketHighlight.unexpectedBracket.foreground": "#FF3C38",

    "activityBar.background": "#26122F",
    "activityBar.foreground": "#FFD1E3",
    "activityBarBadge.background": "#FF3CA6",

    "sideBar.background": "#24112B",
    "sideBar.foreground": "#E6E1E8",
    "sideBarTitle.foreground": "#FF7FA5",
    "sideBarSectionHeader.background": "#2A142F",

    "statusBar.background": "#1C0A26",
    "statusBar.foreground": "#FFD1E3",
    "statusBar.noFolderBackground": "#1B0B2A",
    "statusBar.debuggingBackground": "#FFD75F",
    "statusBar.debuggingForeground": "#1B0B2A",

    "titleBar.activeBackground": "#24112B",
    "titleBar.activeForeground": "#FFD1E3",

    "tab.activeBackground": "#301040",
    "tab.activeForeground": "#FF5F7F",
    "tab.inactiveBackground": "#1B0B2A",
    "tab.inactiveForeground": "#A98BA0",
    "tab.hoverBackground": "#FF5F7F33",
    "tab.unfocusedHoverBackground": "#FFD75F33",
    "tab.activeBorder": "#FF3CA6",
    "tab.inactiveHoverBackground": "#A98BA022",

    "panel.background": "#24112B",
    "panelTitle.activeBorder": "#FF3CA6",
    "panelTitle.activeForeground": "#FF5F7F",

    "terminal.background": "#1B0B2A",
    "terminal.foreground": "#E6E1E8",
    "terminalCursor.background": "#FF3CA6",
    "terminalCursor.foreground": "#1B0B2A",

    "minimap.background": "#1B0B2A",
    "minimap.selectionHighlight": "#FF5F7FCC",

    "errorForeground": "#FF3C38",

    "gitDecoration.addedResourceForeground": "#FFD75F",
    "gitDecoration.modifiedResourceForeground": "#FF7FA5",
    "gitDecoration.deletedResourceForeground": "#FF3C38",
    "gitDecoration.renamedResourceForeground": "#FF3CA6",
    "gitDecoration.untrackedResourceForeground": "#FF5F7F",

    "dropdown.background": "#2A142F",
    "dropdown.foreground": "#E6E1E8",
    "dropdown.listBackground": "#24112B",
    "dropdown.listForeground": "#E6E1E8",
    "dropdown.listHoverBackground": "#FF5F7F44",

    "menu.background": "#2A142F",
    "menu.foreground": "#E6E1E8",
    "menu.selectionBackground": "#FF3CA644",
    "menu.selectionForeground": "#FFFFFF",

    "button.background": "#2A142F",
    "button.foreground": "#FFD1E3",
    "button.hoverBackground": "#FF3CA644",
    "button.secondaryBackground": "#24112B",
    "button.secondaryHoverBackground": "#FFD75F33",

    "input.background": "#2E152F",
    "input.foreground": "#FFD1E3",
    "input.border": "#FF5F7F",
    "input.placeholderForeground": "#FF9FC299",
    "input.focusBorder": "#FF3CA6",

    "focusBorder": "#FF3CA6",

    "inputOption.activeBackground": "#FF3CA644",
    "inputOption.activeBorder": "#FF3CA6",
    "inputOption.activeForeground": "#FFFFFF",

    "inputValidation.errorBackground": "#2A0E2A",
    "inputValidation.errorBorder": "#FF3C38",
    "inputValidation.warningBackground": "#2A0E2A",
    "inputValidation.warningBorder": "#FFD75F",
    "inputValidation.infoBackground": "#2A0E2A",
    "inputValidation.infoBorder": "#FF5F7F",

    "editorWidget.background": "#2A142F",
    "editorWidget.border": "#FF3CA6",
    "editorWidget.foreground": "#E6E1E8",
    "editorWidget.resizeBorder": "#FFD75F",

    "textLink.foreground": "#FF7FA5",
    "textLink.activeForeground": "#FF3CA6",

    "progressBar.background": "#FF3CA6"
  },

  "tokenColors": [
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#FF5F7F",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#FFD75F"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#FFB84F"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#FF9F8A"
      }
    },
    {
      "scope": "function",
      "settings": {
        "foreground": "#FF3CA6"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#A98BA0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#FFB84F"
      }
    },
    {
      "scope": "type",
      "settings": {
        "foreground": "#FF7FA5"
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#FF3CA6"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#FFD1E3"
      }
    },
    {
      "scope": "operator",
      "settings": {
        "foreground": "#FF7FA5"
      }
    }
  ]
}
